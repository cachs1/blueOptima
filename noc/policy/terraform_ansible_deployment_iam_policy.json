{
  "Version": "2021-03-09",
  "Statement": [
    {
      "Sid": "CustomPolicy",
      "Action": [
        "ec2:Describe*",
        "ec2:Get*",
        "s3:CreateBucket",
        "s3:DeleteBucket",
        "s3:DeleteObject",
        "s3:GetBucketLocation",
        "s3:GetObject",
        "s3:HeadBucket",
        "s3:ListBucket",
        "s3:PutObject"
      ],
      "Effect": "Allow",
      "Resource": "*"
    }
  ]
}
